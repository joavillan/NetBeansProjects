query.kocreacion.bucket=SELECT * FROM MOMA_RULES_APLICABILITY WHERE ID_RULE\=124 AND APLICABILITY_VALUE \= {VALUE}
query.kocreacion1=SELECT WORK_ORDER , MIN(ENVELOPE_ID), MAX(ENVELOPE_ID) FROM MOMA_TRACE_LOG WHERE WORK_ORDER IN (SELECT WORK_ORDER FROM MOMA_TRACE_LOG WHERE ENVELOPE_ID LIKE 'MOMAES000001%' AND HAS_EXCEPTIONS \= 1 AND TRACE_DATE>\='{LASTDATE}') AND ENVELOPE_ID LIKE 'MOMAES000001%' AND WORK_ORDER NOT IN ({EXCLUIDOS}) GROUP BY WORK_ORDER HAVING MAX(ENVELOPE_ID) <> 'MOMAES0000010700' AND MIN(ENVELOPE_ID) <> 'MOMAES0000010001' ORDER BY MAX(ENVELOPE_ID),WORK_ORDER
query.kocreacion1.2=SELECT WORK_ORDER , MIN(ENVELOPE_ID), MAX(ENVELOPE_ID) FROM MOMA_TRACE_LOG WHERE WORK_ORDER IN (SELECT WORK_ORDER FROM MOMA_TRACE_LOG WHERE ENVELOPE_ID LIKE 'MOMAES000001%' AND HAS_EXCEPTIONS \= 1 AND TRACE_DATE>\='{LASTDATE}') AND ENVELOPE_ID LIKE 'MOMAES000001%' GROUP BY WORK_ORDER HAVING MAX(ENVELOPE_ID) <> 'MOMAES0000010700' AND MIN(ENVELOPE_ID) <> 'MOMAES0000010001' ORDER BY MAX(ENVELOPE_ID),WORK_ORDER
query.kocreacion2=SELECT DISTINCT (WORK_ORDER), OPERATION_CODE, MAX(TRACE_DATE), DESCRIPTION FROM MOMA_TRACE_LOG WHERE WORK_ORDER IN ({ORDENES}) AND HAS_EXCEPTIONS=1 GROUP BY WORK_ORDER,OPERATION_CODE,DESCRIPTION ORDER BY MAX(TRACE_DATE) DESC
query.kocreacion.cribaexcluidos=SELECT DISTINCT(WORK_ORDER) FROM MOMA_TRACE_LOG WHERE TRACE_DATE>'{LASTDATE}' AND WORK_ORDER IN ({EXCLUIDOS})
query.kocreacionD.vinculadas=SELECT ID_WORK_ORDER, ASTATUS FROM MOMAKPID.MKPI_TL_MOMACON_ACTIVITY WHERE ID_WORK_ORDER \={IDORDER}
query.kocreacionP.vinculadas=SELECT ID_WORK_ORDER, ASTATUS FROM MOMAKPIP.MKPI_TL_MOMACON_ACTIVITY WHERE ID_WORK_ORDER \={IDORDER}

