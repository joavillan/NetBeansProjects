query.trackactivityA=SELECT * FROM MOMAPICORE.MOMA_TRACK_ACTIVITY WHERE ID_ACTIVITY='{ACTIVITY}'
query.trackorder=SELECT * FROM MOMAPICORE.MOMA_TRACK_ORDER WHERE ID_WORK_ORDER='{ORDER}'
query.pdtesBackend=SELECT ID_WORK_ORDER,UPDATE_DATE,SYSDATE FROM MOMAKPIP.MKPI_TL_MOMACON_ACTIVITY WHERE PROCESS_STATUS='Pdte Backend'
query.cuadreD=SELECT ID_WORK_ORDER,OPERACION_CODE, PROCESS_STATUS, COD_TEAM, UPDATE_DATE, MENSAJE_ERROR_INTEGRACION FROM MOMAKPID.MKPI_TL_MOMACON_ACTIVITY WHERE PROCESS_STATUS \= 'Error BackEnd'
query.cuadre2D=SELECT ID_WORK_ORDER,OPERACION_CODE, PROCESS_STATUS, COD_TEAM, UPDATE_DATE, MENSAJE_ERROR_INTEGRACION FROM MOMAKPID.MKPI_TL_MOMACON_ACTIVITY WHERE PROCESS_STATUS \= 'Error Desconocido ZEUS'
query.cuadreP=SELECT ID_WORK_ORDER,OPERACION_CODE, PROCESS_STATUS, COD_TEAM, UPDATE_DATE, MENSAJE_ERROR_INTEGRACION FROM MOMAKPIP.MKPI_TL_MOMACON_ACTIVITY WHERE PROCESS_STATUS \= 'Error BackEnd'
