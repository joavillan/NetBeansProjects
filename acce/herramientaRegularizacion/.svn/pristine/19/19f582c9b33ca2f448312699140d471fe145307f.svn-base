query.momacon=SELECT ID_WORK_ORDER,APPT_NUMBER,ASTATUS,PROCESS_STATUS,UPDATE_DATE FROM MOMAKPIP.MKPI_TL_MOMACON_ACTIVITY WHERE PROCESS_STATUS NOT IN('Finalizada','Cerrada','Cerrada en backend') AND ID_WORK_ORDER IN({ORDERS})


query.trackactivityO=SELECT * FROM MOMAPICORE.MOMA_TRACK_ACTIVITY WHERE ID_WORK_ORDER='{ORDER}'
